{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ES2015"],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": false,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true,
		"removeComments": false,
		"rootDir": "src",
		"outDir": "./dist"
	},
	"include": ["src/*.ts"]
}
